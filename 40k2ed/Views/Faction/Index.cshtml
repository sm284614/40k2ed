<div class="container">
	<div class="align-content-center">
		<h1>Genres</h1>
	</div>
	<div class="col-11 mx-auto">
		@foreach (Faction faction in Model)
		{
			<a asp-controller="Faction" asp-action="FactionInfo" asp-route-faction_id="@faction.FactionId" class="btn btn-primary w-20 m-1">
				@faction.Name
			</a>
		}
	</div>
</div>